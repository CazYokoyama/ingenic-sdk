ccflags-y += -I$(src)/isp/$(FAMILY)/include

include $(src)/sinfo/Kbuild
include $(src)/gpio/Kbuild
include $(src)/avpu/$(FAMILY)/Kbuild
include $(src)/isp/$(FAMILY)/Kbuild
